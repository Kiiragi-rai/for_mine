<%= form_with model: @anniversary do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<div>
    <%= f.label :title%>
    <%= f.text_field :title%>
</div>
<div>
    <%= f.label :anniversary_date %>
    <%= f.date_field :anniversary_date %>
</div>
<div>
     <%= f.submit nil %>
</div>
<% end %>
<%= link_to "詳細", anniversary_path(@anniversary) %>