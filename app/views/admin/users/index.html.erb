<h1> Users</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>id</th>
            <th>neme</th>
            <th>date</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <td><%= user.created_at %></td>
        </tr>
        <% end %>
    </tbody>
</table>